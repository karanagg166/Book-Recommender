{
  "version": 2,
  "buildCommand": "./build.sh",
  "outputDirectory": "frontend/.next",
  "installCommand": "echo 'Dependencies will be installed during build'",
  "functions": {
    "backend/api/index.py": {
      "maxDuration": 30,
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
} 